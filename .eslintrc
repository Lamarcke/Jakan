{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "ignorePatterns": [
    "dist",
    "node_modules"
  ],
  "rules": {
    "no-console": 2,
    // Means error
    "prettier/prettier": 1
    // Means warning
  }
}